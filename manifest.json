{
  "name": "Tab URLs",
  "version": "1.2",
  "manifest_version": 3,
  "description": "Displays the URLs of all open tabs.",
  "permissions": ["tabs", "activeTab"],
  "icons": {
    "16": "link16.png",
    "32": "link32.png",
    "48": "link48.png",
    "128": "link128.png"
  },
  "action": {
    "default_icon": "link.png",
    "default_popup": "popup.html"
  }
}