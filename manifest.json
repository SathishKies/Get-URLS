{
  "name": "Tab URLs",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Displays the URLs of all open tabs.",
  "permissions": ["tabs", "activeTab"],
  "icons": {
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}